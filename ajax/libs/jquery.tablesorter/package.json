    "name": "Christian Bach",
    "url": "http://tablesorter.com/"
  },
  "keywords": [
    "jquery.tablesorter",
    "tablesorter",
    "table",
    "sort",
    "sorter",
    "sorting",
    "alphanumeric",
    "natural"
  ],
  "licenses": [
    "MIT",
    {
      "type": "GPL",
      "url": "http://www.gnu.org/licenses/gpl.html"
    }
  ],
  "homepage": "http://mottie.github.com/tablesorter/",
  "docs": "http://mottie.github.com/tablesorter/docs/index.html",
  "demo": "https://github.com/Mottie/tablesorter/wiki",
  "dependencies": {
    "jquery": ">=1.2.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Mottie/tablesorter.git"
  },
  "autoupdate": {
    "source": "git",
    "target": "git://github.com/Mottie/tablesorter.git",
    "basePath": "dist",
    "files": [
      "**/*"
    ]
  }
